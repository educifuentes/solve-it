<div class="category-container-show">
  <div class="services-container">
    <br>
    <h2 id="category-name"><%= @category.name %>s  <i id="category-icon" class="<%= @category.icon %>  icon-home" ></i></h2>
    <h2>What problem are you having?</h2>
    <br>
    <div class="service">
      <% @category.services.each do |service| %>
      <ul>
        <li id="service-name"><%= link_to service.name, services_technicians_path(:service_id => service.id) %></li>
      </ul>
    <% end %>
    </div>
  </div>
</div>
